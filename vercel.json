{
  "version": 2,
  "builds": [
    {
      "src": "server-with-auth.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/server-with-auth.js"
    },
    {
      "src": "/auth/(.*)",
      "dest": "/server-with-auth.js"
    },
    {
      "src": "/dashboard",
      "dest": "/server-with-auth.js"
    },
    {
      "src": "/login",
      "dest": "/server-with-auth.js"
    },
    {
      "src": "/register",
      "dest": "/server-with-auth.js"
    },
    {
      "src": "/(.*)",
      "dest": "/server-with-auth.js"
    }
  ]
}
